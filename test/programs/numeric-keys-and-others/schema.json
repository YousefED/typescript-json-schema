{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "patternProperties": {
        "^[0-9]+$": {
            "type": "number"
        }
    },
    "properties": {
        "a": {
            "type": "string"
        },
        "b": {
            "type": "boolean"
        }
    },
    "required": [
        "a",
        "b"
    ],
    "type": "object"
}