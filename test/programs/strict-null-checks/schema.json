{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "val": {
      "type": "number"
    },
    "valNullable": {
      "type": [ "null", "number" ]
    },
    "valOpt": {
      "type": "number"
    },
    "valTrue": {
      "const": true
    },
    "valTrueOpt": {
      "const": true
    },
    "valTrueOrNull": {
      "anyOf": [
        {
          "const": true
        },
        {
          "type": "null"
        }
      ]
    },
    "valUndef": {
      "type": "number"
    },
    "valVoid": {
      "type": "number"
    }
  },
  "required": [ "val", "valNullable", "valTrue", "valTrueOrNull" ],
  "type": "object"
}
